@startuml
!include style.puml
mainframe sd get FindPersonPredicate

box Logic LOGIC_COLOR_T1
participant ":FindCommandParser" as FindCommandParser LOGIC_COLOR
participant "descriptor:PersonDescriptor" as PersonDescriptor LOGIC_COLOR
participant "predicate:FindPersonPredicate" as FindPersonPredicate LOGIC_COLOR
end box


activate FindCommandParser
FindCommandParser -> FindCommandParser : getDescriptor("n/alice")
activate FindCommandParser

create PersonDescriptor
FindCommandParser -> PersonDescriptor : PersonDescriptor("n/alice")
activate PersonDescriptor

PersonDescriptor --> FindCommandParser : descriptor
deactivate PersonDescriptor
FindCommandParser --> FindCommandParser : descriptor
deactivate FindCommandParser

create FindPersonPredicate
FindCommandParser -> FindPersonPredicate : FindPersonPredicate(descriptor)
activate FindPersonPredicate
FindPersonPredicate --> FindCommandParser : predicate
@enduml
