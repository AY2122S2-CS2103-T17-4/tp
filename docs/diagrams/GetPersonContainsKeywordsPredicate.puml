@startuml
!include style.puml
mainframe sd get PersonContainsKeywordsPredicate

box Logic LOGIC_COLOR_T1
participant ":FindCommandParser" as FindCommandParser LOGIC_COLOR
participant ":ArgumentTokenizer" as ArgumentTokenizer LOGIC_COLOR
participant "argMultimap:ArgumentMultimap" as ArgumentMultimap LOGIC_COLOR
participant "predicate:PersonContainsKeywordsPredicate" as PersonContainsKeywordsPredicate LOGIC_COLOR
end box


activate FindCommandParser
FindCommandParser -> FindCommandParser : createArgumentMultimap("find n/alice")
activate FindCommandParser

FindCommandParser -> ArgumentTokenizer : tokenize("find n/alice")
activate ArgumentTokenizer

ArgumentTokenizer -> ArgumentTokenizer : extractArguments("find n/alice", Prefix...)
activate ArgumentTokenizer

create ArgumentMultimap
ArgumentTokenizer -> ArgumentMultimap
activate ArgumentMultimap


ArgumentMultimap -->  ArgumentTokenizer : argMultimap
deactivate ArgumentMultimap

ArgumentTokenizer --> ArgumentTokenizer : argMultimap
deactivate ArgumentTokenizer

ArgumentTokenizer --> FindCommandParser : argMultimap
deactivate ArgumentTokenizer


FindCommandParser -> ArgumentMultimap : removePreamble()
activate ArgumentMultimap
ArgumentMultimap --> FindCommandParser : argMultimap
deactivate ArgumentMultimap

FindCommandParser --> FindCommandParser
deactivate FindCommandParser

create PersonContainsKeywordsPredicate
FindCommandParser -> PersonContainsKeywordsPredicate : PersonContainsKeywordsPredicate(argMultimap)
activate PersonContainsKeywordsPredicate

PersonContainsKeywordsPredicate --> FindCommandParser : predicate
deactivate PersonContainsKeywordsPredicate

@enduml
